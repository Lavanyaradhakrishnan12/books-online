
<div class="content4">
    <%- include("partials/header.ejs") %>    
    <div class="login">
        <form action="/login" method="post">
            <h2>Login</h2>
            <label for="name">Username</label>
            <input type="email" id="email" name="username" placeholder="Enter Your Username" autocomplete="off" autofocus="off" />
            <label for="password">Password</label>
            <input type="password" id="password" name="password" placeholder="Enter Your Password" autocomplete="off" autofocus="off">
            <button type="submit">Submit</button>
        </form>
    </div>

</div>